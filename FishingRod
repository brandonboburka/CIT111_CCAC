public class FishingRod {
    // type of bait used to catch the fish
    String baitType;
    // how far the hook was thrown in to the lake
    int castDistance;
    // type of fish caught
    String fishCaught;
    // how big the fish was
    int fishLength;
    
    
    
} // close class
